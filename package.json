{
  "name": "@molgenis/catalogue-exporter",
  "version": "0.1.0",
  "license": "LGPL-3.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Sido Haakma, Fleur Kelpin",
  "description": "Service to export the MOLGENIS catalogue model to EMX",
  "repository": "https://github.com/molgenis/molgenis-js-catalogue-exporter",
  "keywords": [
    "catalogue",
    "export"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/molgenis-js-catalogue-exporter.esm.js",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.0.8",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "prettier-config-standard": "^1.0.1",
    "standard": "^14.3.4",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/jszip": "^3.4.1",
    "@types/lodash.groupby": "^4.6.6",
    "@types/node-fetch": "^2.5.7",
    "csv-stringify": "^5.5.0",
    "jszip": "^3.4.0",
    "lodash.groupby": "^4.6.0",
    "node-fetch": "^2.6.1",
    "xlsx": "^0.16.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
